[Version]
version_major=@version_major@
version_minor=@version_minor@
version_patch=@version_patch@

[common]
logic_thread_share=0.6
graph_thread_share=0.4
manager_size=100000 ; The maximum number of objects that the management system can manage.
manager_size_asset_manager=100000 ; Specify an independent maximum number of objects that can be managed for each management system.If the item is not specified, use “manager_size“.
manager_size_render_resource_manager=100000 ; Specify an independent maximum number of objects that can be managed for each management system.If the item is not specified, use “manager_size“.

[Path]
engine_dir=@engine_dir@
config_dir=@config_dir@
bin_dir=@bin_dir@
asset_dir=@asset_dir@
asset_dir_std=@asset_dir_std@
asset_dir_user=@asset_dir_user@

[RenderConfig]
window_extent_width=1920
window_extent_height=1080
multi_sample_count=4
init_vertex_buffer_size=131072000
max_vertex_buffer_size=1073741760
init_index_buffer_size=8192000
max_index_buffer_size=32768000
